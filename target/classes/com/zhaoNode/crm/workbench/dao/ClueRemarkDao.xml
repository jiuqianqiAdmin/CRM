<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhaoNode.crm.workbench.dao.ClueRemarkDao">
  	<select id="getRemarkByCoulId" resultType="ClueRemark">
		select * from tbl_clue_remark where clueid=#{clueId}
	</select>
	<delete id="deleteByClueId">
		delete from tbl_clue_remark where  clueid=#{clueid}
	</delete>
</mapper>

































